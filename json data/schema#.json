{
    "$schema": "./schema.json",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "username",
            "email": "example@example.com",
            "location": "Place",
            "listing": {
                "id": 1,
                "posted_date": "DD/MM/YYY",
                "title": "Example",
                "price": "$xx.xx"
            }
        }
    ],
    "required": [
        "username",
        "email",
        "location",
        "listing"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "default": "",
            "description": "Username for website user.",
            "examples": [
                "username"
            ],
            "title": "The username schema",
            "type": "string"
        },
        "email": {
            "$id": "#/properties/email",
            "default": "example@example.com",
            "description": "User's email address",
            "examples": [
                "example@example.com"
            ],
            "title": "The email schema",
            "type": "string"
        },
        "location": {
            "$id": "#/properties/location",
            "default": "Earth",
            "description": "Users location",
            "examples": [
                "Earth"
            ],
            "title": "The location schema",
            "type": "string"
        },
        "listing": {
            "$id": "#/properties/listing",
            "default": {},
            "description": "Information about the users listings",
            "examples": [
                {
                    "id": 1,
                    "posted_date": "DD/MM/YYY",
                    "title": "Example",
                    "price": "$xx.xx"
                }
            ],
            "required": [
                "id",
                "posted_date",
                "title",
                "price"
            ],
            "title": "The listing schema",
            "type": "object",
            "properties": {
                "id": {
                    "$id": "#/properties/listing/properties/id",
                    "type": "integer",
                    "title": "The id schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        1
                    ]
                },
                "posted_date": {
                    "$id": "#/properties/listing/properties/posted_date",
                    "default": "DD/MM/YYYY",
                    "description": "Date user posted listing",
                    "examples": [
                        "DD/MM/YYY"
                    ],
                    "title": "The posted_date schema",
                    "type": "string"
                },
                "title": {
                    "$id": "#/properties/listing/properties/title",
                    "default": "Choose a title",
                    "description": "Users chosen title for listing",
                    "examples": [
                        "Example"
                    ],
                    "title": "The title schema",
                    "type": "string"
                },
                "price": {
                    "$id": "#/properties/listing/properties/price",
                    "default": "$xx.xx",
                    "description": "How much the user is selling their item for",
                    "examples": [
                        "$xx.xx"
                    ],
                    "title": "The price schema",
                    "type": "string"
                }
            },
            "additionalProperties": true
        }
    },
    "additionalProperties": true
}